    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "graphql": "^15.4.0",
    "jsonwebtoken": "^8.5.1",
    "logger": "0.0.1",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
